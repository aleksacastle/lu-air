
<%= form_for "/scraper/new", method: "post" do |f|%>
  <%= f.label "Street address"%>
  <%= f.text_field :street  %>
  <%= f.label "City" %>
  <%= f.text_field :city  %>
  <%= f.label "Country"%>
  <%= f.text_field :country  %>
  <%= f.label "Long-term rent"%>
  <%= f.text_field :long_term_rent  %>
  <%= f.submit("Show") %>
<% end %>
